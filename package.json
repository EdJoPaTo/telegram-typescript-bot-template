{
	"name": "telegram-typescript-bot-template",
	"private": true,
	"version": "0.0.0",
	"description": "",
	"keywords": [
		""
	],
	"license": "MIT",
	"repository": "<%= githubUsername %>/telegram-typescript-bot-template",
	"author": {
		"name": "<%= name %>",
		"email": "<%= email %>",
		"url": "<%= website %>"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"start": "tsc --sourceMap && node --unhandled-rejections=strict -r source-map-support/register dist/source/index.js",
		"test": "tsc --sourceMap && xo && nyc ava"
	},
	"engines": {
		"node": ">=14"
	},
	"dependencies": {
		"@edjopato/telegraf-i18n": "^0.1.0",
		"source-map-support": "^0.5.19",
		"telegraf": "^3.38.0",
		"telegraf-inline-menu": "^5.0.0",
		"telegraf-session-local": "^2.0.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/node": "^14.0.5",
		"ava": "^3.0.0",
		"del-cli": "^3.0.0",
		"nyc": "^15.0.0",
		"typescript": "^4.0.2",
		"xo": "^0.35.0"
	},
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"main": "dist/source",
	"types": "dist/source",
	"nyc": {
		"all": true,
		"extension": [
			".ts"
		],
		"include": [
			"dist/source",
			"source"
		],
		"exclude": [
			"**/*.d.ts",
			"**/*.test.ts"
		],
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"ava/no-ignored-test-files": "off"
		}
	}
}
